{
    "$schema": "./mirrors.schema.json",
    "mirrors": [
        {
            "name": "Test",
            "short": "test",
            "isDistro": true,
            "rsync": {
                "host": "mirror.clarkson.edu",
                "src": "test",
                "dest": "/tmp/test",
                "options": "-avzrH --numeric-ids --delete --delete-delay --delay-updates --exclude \".~tmp~\"",
                "syncs_per_day": 8
            },
            "official": false,
            "homepage": "https://mirror.clarkson.edu"
        }
    ]
}